<ul>
  <li>
    <% @spots.each do |spot|%>
      <%= spot.name%>
      <%= image_tag spot.spot_images[0].src.url, width: "300px", height:"300px"%>
    <% end%>
  </li>
</ul>

<style>
#map {
  height: 400px;
  width: 600px;
}
</style>

<div id="map"></div>
<script>
  var map;
  function initMap() {
    map = new google.maps.Map(document.getElementById('map'), {
      center: {lat: -34.397, lng: 150.644},
      zoom: 8
    });
  }
</script> 